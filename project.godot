config_version=5

[application]

config/name="FFTorama"
config/description="Pixelorama extension to show assembled frames and animations from spritesheets compatible with Final Fantasy Tactics."
config/version="0.01"
run/main_scene="res://src/Extensions/FFTorama/Main.tscn"
config/features=PackedStringArray("4.2", "C#", "GL Compatibility")
config/icon="res://icon.svg"

[dotnet]

project/assembly_name="FFTorama"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
